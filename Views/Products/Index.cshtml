@model IEnumerable<technicalTestCrud.Models.Product>

<h2>Products</h2>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th>Price</th>
    </tr>
  </thead>
  <tbody>
    @foreach (var item in Model)
    {
      <tr>
        <td>@item.Id</td>
        <td>@item.Name</td>
        <td>@item.Price</td>
      </tr>
    }
  </tbody>
</table>